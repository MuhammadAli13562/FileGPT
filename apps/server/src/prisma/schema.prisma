
generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/taxprogpt?schema=public"
}


model User {

  Id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name String
  email String @unique
  passwordHash String
  contextWindows Context_Window[]

}

model Context_Window {

  Id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  chatEngineMessages Json
  ChatWindowMessages  Chat_Message[]
  vectorURL String
  fileName String
  fileURL String
  fileKey String
  owner User @relation(fields: [ownerId] , references: [Id] , onUpdate: Cascade , onDelete: Cascade)
  ownerId Int

}

model Chat_Message {
  Id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  content String
  role String
  ContextWindow Context_Window @relation(fields: [ContextWindowId] , references: [Id] , onUpdate: Cascade , onDelete: Cascade)
  ContextWindowId Int
}

